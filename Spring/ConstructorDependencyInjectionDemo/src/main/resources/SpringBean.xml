<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person" class="org.example.Person">
       <constructor-arg value="Saialakshmi"/>
        <constructor-arg value="23/12/2000"/>
    </bean>

    <bean id="emp" class="org.example.Employee">
       <constructor-arg value="101" type="int"/>
        <constructor-arg value="JAVA DEVELOPER" />
        <constructor-arg ref="person"/>
    </bean>

</beans>
